{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: github.py (Debug clean)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/github.py",
      "cwd": "${workspaceFolder}",
      "console": "internalConsole",
      "justMyCode": true,
      "subProcess": false
    },
    {
      "name": "Python: Current file (clean)",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "cwd": "${workspaceFolder}",
      "console": "internalConsole",
      "justMyCode": true,
      "subProcess": false
    }
  ]
}
